  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/main/server.js",
    "test": "jest --passWithNoTests --silent --noStackTrace --maxWorkers=25%",
    "test:verbose": "jest --passWithNoTests --runInBand --detectOpenHandles",
    "test:unit": "yarn test --watch --onlyChanged -c jest-unit-config.js",
    "test:integration": "yarn test --watch -c jest-integration-config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "jest --clearCache && yarn test --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@shelf/jest-mongodb": "^2.0.3",
    "@types/babel__core": "^7.1.16",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
