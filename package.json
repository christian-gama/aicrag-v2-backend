{
  "name": "backend",
  "version": "4.1.3",
  "main": "./src/main/express/server.js",
  "author": "Christian Gama <christiangama.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "babel:watch": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored --watch",
    "build": "rimraf dist && pnpm run codegen && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "codegen": "rimraf src/main/graphql/generated && graphql-codegen",
    "compose:down": "docker-compose down",
    "compose:restart": "docker-compose restart aicrag_dev",
    "compose:up": "docker-compose up --build -d",
    "coveralls": "cat ./coverage/locv.info | coveralls",
    "dev:docker": "concurrently \"pnpm run babel:watch\" \"cross-env NODE_ENV=development nodemon -L -e '*' --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/express/server.js\"",
    "dev:local": "pnpm run build && cross-env PORT=4001 NODE_ENV=development MONGO_URL=mongodb://localhost:27017/aicrag nodemon -e '*' --watch ./dist --inspect --nolazy ./dist/main/express/server.js",
    "format": "prettier --write . && eslint . --fix",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production node dist/main/express/server.js",
    "test:ci": "pnpm run test -- --coverage",
    "test:commit": "pnpm run test:silent -- --bail --findRelatedTests",
    "test:integration": "pnpm run test:verbose -- -c jest-integration-config.js",
    "test:silent": "jest --passWithNoTests --silent --noStackTrace",
    "test:unit": "pnpm run test:silent -- -c jest-unit-config.js",
    "test:verbose": "jest --passWithNoTests --detectOpenHandles --verbose",
    "test": "jest --clearCache && pnpm run test:silent -- -c jest-ci-config.js"
  },
  "devDependencies": {
